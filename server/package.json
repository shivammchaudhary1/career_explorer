{
  "name": "career-explorer",
  "version": "1.0.0",
  "description": "Career Explorer Backend",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": "20.*",
    "npm": ">=10"
  },
  "scripts": {
    "server": "nodemon server.js",
    "format": "npm run -- prettier --write",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prettier": "prettier '**/*.{cjs,js,json,html,md,yml}'"
  },
  "imports": {
    "##/*": "./*"
  },
  "lint-staged": {
    "*.{,c}js": [
      "prettier --write",
      "eslint"
    ],
    "*.{json,html,md,yml}": "prettier --write"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.521.0",
    "@eslint/js": "^8.56.0",
    "aws-sdk": "^2.1566.0",
    "bad-words": "^3.0.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "bullmq": "^5.3.1",
    "chalk": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-unicorn": "^51.0.1",
    "express": "^4.18.2",
    "globals": "^14.0.0",
    "helmet": "^7.1.0",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.10",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "stripe": "^15.12.0",
    "uuid": "^11.0.5"
  }
}
